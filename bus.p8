pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
actors={} -- array of actors
speed=2 -- bus speed
kids={} -- array of kids
num_kids=10 -- number of kids
kid_wid=4 -- width of circle

-- currently only makes a bus
function make_actor(x,y)
 a={}
 a.x=x -- x pos
 a.y=y -- y pos
 a.xvel=0 -- x velocity
 a.yvel=0 -- y velocity
 a.spr=0 -- default sprite
 add(actor,a)
 return a
end

-- every kid = circle
function make_kid()
 k={}
 k.x=rnd(120)+kid_wid -- random x pos such that circle is in screen
 k.y=rnd(120)+kid_wid -- random y pos such that circle is in screen
 k.c=flr(rnd(15))+1 -- random color
 add(kids,k)
 return k
end

function control_bus()
 btnpressx=false
 btnpressy=false
 -- left
 if(btn(0)) then
  bus.xvel=-speed
  bus.spr=5
  btnpressx=true
 end
 -- right
 if(btn(1)) then
  bus.xvel=speed
  bus.spr=7
  btnpressx=true
 end
 -- up
 if(btn(2)) then
  bus.yvel=-speed
  bus.spr=1
  btnpressy=true
 end
 -- down
 if(btn(3)) then
  bus.yvel=speed
  bus.spr=3
  btnpressy=true
 end
 -- friction
 if not(btnpressx) then
  bus.xvel=bus.xvel/1.75
 end
 if not(btnpressy) then
  bus.yvel=bus.yvel/1.75
 end
 -- bus goes zoom-zoom
 bus.x+=bus.xvel
 bus.y+=bus.yvel
end

function draw_bus()
 spr(bus.spr,bus.x,bus.y,2,2)
end

function draw_kids(k)
 circfill(k.x,k.y,kid_wid,k.c)
end

function _init()
 bus=make_actor(8,8)
 bus.spr=1
 for i=1,num_kids do
  make_kid()
 end
end

function _update()
 control_bus()
 pal(flr(rnd(14))+1,flr(rnd(14))+1)
end

function _draw()
 cls()
 foreach(kids,draw_kids)
 draw_bus()
end
